	<CheckBoxPreference 
	android:title="5、心脑血管"
	android:key="disease_05"
	/>
	<CheckBoxPreference 
	android:title="6、心脏病"
	android:key="disease_06"
	/>
	<CheckBoxPreference 
	android:title="7、肝胆疾病"
	android:key="disease_07"
	/>
	<CheckBoxPreference 
	android:title="8、肾脏疾病"
	android:key="disease_08"
	/>
	<CheckBoxPreference 
	android:title="9、泌尿生殖系统"
	android:key="disease_09"
	/>
	<CheckBoxPreference 
	android:title="10、骨科疾病"
	android:key="disease_10"
	/>
