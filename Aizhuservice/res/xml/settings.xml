	android:title="11、健康"
	android:key="disease_11"
	/>
	<CheckBoxPreference 
	android:title="12、其他"
	android:key="disease_12"
	/>
	<PreferenceCategory 
		android:title="服务项目（必填）"
		android:summary="服务项目">
	</PreferenceCategory>
	<CheckBoxPreference 
		android:key="insurance_01" 
		android:title="咨询">
	</CheckBoxPreference>
	<CheckBoxPreference 
		android:key="insurance_02" 
		android:title="紧急救援">
	</CheckBoxPreference>
